{
  "name": "PP",
  "type": "light",
  "colors": {
      "editor.background": "#EDE9E0",
      "editor.selectionBackground": "#D8D4CC",
      "editor.selectionHighlightBackground": "#D8D4CC",
      "editor.lineHighlightBackground": "#F1EFE8",
      "editor.lineHighlightBorder": "#F1EFE8",
      "editor.inactiveSelectionBackground": "#D8D4CC",
      "editor.wordHighlightBackground": "#D8D4CC",
      "editor.wordHighlightStrongBackground": "#D8D4CC",
      "editor.findMatchBackground": "#D8D4CC",
      "editor.findMatchHighlightBackground": "#D8D4CC",
      "editor.findRangeHighlightBackground,": "#D8D4CC",
      "editorIndentGuide.background": "#D8D4CC",
      "editorLineNumber.foreground": "#8D9697",
      "editorGutter.modifiedBackground": "#8D9697",
      "editorGutter.addedBackground": "#339033",
      "editorGutter.deletedBackground": "#EB444F",
      "editorOverviewRuler.modifiedBackground": "#8D9697",
      "editorOverviewRuler.addedBackground": "#339033",
      "editorOverviewRuler.deletedBackground": "#EB444F"
  },
  "tokenColors": [
    {
        "settings": {
          "foreground": "#000000",
          "background": "#EDE9E0"
        }
    },
    {
      "name": "Black",
      "scope": [
        "entity.name",
        "entity.other.inherited-class",
        "entity.name.section",
        "entity.name.tag",
        "entity.other.attribute-name",
        "variable",
        "variable.language",
        "variable.parameter",
        "variable.function",
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character.escape",
        "keyword",
        "keyword.control",
        "support"        
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Grey",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8D9697"
      }
    },
    {
      "name": "Red",
      "scope": [
        "punctuation",
        "keyword.operator",
        "meta.tag.structure.any",
        "meta.tag.inline.any"
      ],
      "settings": {
        "foreground": "#EB444F"
      }
    },
    {
      "name": "Green",
      "scope": [
        "string",
        "punctuation.definition.string",
        "punctuation.support.type.property-name",
        "support.type.property-name",
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#339033"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1E73BE"
      }
    }
  ]
}